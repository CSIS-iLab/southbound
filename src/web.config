<?xml version="1.0"?>
  <configuration>
    <system.webServer>
      <staticContent>
         <clientCache cacheControlCustom="public" cacheControlMode="UseMaxAge" cacheControlMaxAge="365.00:00:00" />
       </staticContent>
      <httpProtocol>
        <customHeaders>
          <add name="X-Frame-Options" value="sameorigin" />
        </customHeaders>
      </httpProtocol>

    </system.webServer>
  </configuration>
